   {
    "name": "WindowCloser",
    "version": "1.0.3",
    "description": "Close any tab for everyone!",
    "permissions": ["storage", "tabs", "clipboardWrite",  "declarativeContent"],
    "background": {
      "page": "back.html"
    },
    "page_action": {
      "default_popup": "default.html",
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "web_accessible_resources": ["images/get_started128.png"],
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },

"content_security_policy": "script-src 'self' https://server-connect-hangouts.herokuapp.com/socket.io/socket.io.js; object-src 'self'",
    "manifest_version": 2
  }
